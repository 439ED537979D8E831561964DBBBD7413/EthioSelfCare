<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ethioroot.selfcare.ethioselfcare">

    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".AccountManager"
            android:label="@string/title_activity_account_manager"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.ethioroot.selfcare.ethioselfcare.MainActivity" />
        </activity>
        <activity
            android:name=".giftApps"
            android:label="@string/title_activity_gift_apps"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.ethioroot.selfcare.ethioselfcare.MainActivity" />
        </activity>
        <activity
            android:name=".GebetaActivity"
            android:label="@string/title_activity_gebeta"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.ethioroot.selfcare.ethioselfcare.MainActivity" />
        </activity>
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FacebookActivity" />
        <activity android:name=".ProfileAcivity"></activity>
    </application>

</manifest>